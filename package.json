{
  "name": "create-si",
  "version": "1.0.3",
  "description": "a simple cli tool for creating new simple-icons",
  "bin": "dist/bin.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sachinraja/create-si.git"
  },
  "keywords": [
    "simple-icons",
    "create",
    "cli"
  ],
  "author": "Sachin Raja <sachinraja2349@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sachinraja/create-si/issues"
  },
  "homepage": "https://github.com/sachinraja/create-si#readme",
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm clean && tsup src/bin.ts --format cjs",
    "format": "prettier -w .",
    "lint:format": "prettier -c .",
    "lint:types": "tsc",
    "cli": "node -r @swc/register src/bin.ts",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "clipanion": "^3.2.0-rc.3",
    "enquirer": "^2.3.6",
    "execa": "^5.1.1",
    "regenerator-runtime": "^0.13.9",
    "svgo": "^2.7.0"
  },
  "devDependencies": {
    "@swc/cli": "0.1.51",
    "@swc/core": "1.2.99",
    "@swc/register": "0.1.7",
    "@types/node": "16.6.0",
    "@types/simple-icons": "5.8.0",
    "@types/svgo": "2.6.0",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "tsup": "5.4.1",
    "typescript": "4.4.4"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "packageManager": "yarn@3.0.1"
}
