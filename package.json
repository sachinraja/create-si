{
  "name": "create-si",
  "version": "1.0.2",
  "description": "a simple cli tool for creating new simple-icons",
  "bin": "dist/bin.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sachinraja/create-si.git"
  },
  "keywords": [
    "simple-icons",
    "create",
    "cli"
  ],
  "author": "Sachin Raja <sachinraja2349@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sachinraja/create-si/issues"
  },
  "homepage": "https://github.com/sachinraja/create-si#readme",
  "scripts": {
    "build": "rimraf dist && swc src -d dist",
    "format": "prettier --write .",
    "type-check": "tsc",
    "type-gen": "tsc -p tsconfig.build.json",
    "cli": "node -r @swc/register src/bin.ts",
    "prepack": "yarn build"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "clipanion": "^3.0.1",
    "enquirer": "^2.3.6",
    "execa": "^5.1.1",
    "regenerator-runtime": "^0.13.9",
    "svgo": "^2.3.1"
  },
  "devDependencies": {
    "@swc/cli": "0.1.46",
    "@swc/core": "1.2.76",
    "@swc/register": "0.1.7",
    "@types/node": "16.6.0",
    "@types/svgo": "2.3.1",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "typescript": "4.3.5"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "packageManager": "yarn@3.0.1"
}
